<section class="channels">
    <h1 class="h2"><%= t '.title', provider: @channel.pretty_provider %></h1>
    <div class="box">
        <div class="box-bd">
            <%= form_for @channel, as: :channel, url: messaging_channel_path(@channel), multipart: true do |f| %>
                <%= render 'form', f: f %>
                <%= link_to t('actions.delete'), messaging_channel_path(@channel), method: :delete, class: 'butn delete', data: {confirm: t('defaults.sure')} %>
            <% end %>
        </div>
    </div>
</section>
